<odoo>

    <!-- Recently -->
    <record id="asterisk_plus_phone_recently_call_user" model="ir.model.access">
        <field name="name">asterisk_plus_phone_recently_call_user</field>
        <field name="model_id" ref="asterisk_plus_phone.model_asterisk_plus_phone_recently_call"/>
        <field name="group_id" ref="asterisk_plus.group_asterisk_user"/>
        <field name="perm_read" eval="1"/>
        <field name="perm_write" eval="1"/>
        <field name="perm_create" eval="1"/>
        <field name="perm_unlink" eval="1"/>
    </record>

    <!-- Favorite -->
    <record id="asterisk_plus_phone_favorite_user" model="ir.model.access">
        <field name="name">asterisk_plus_phone_favorite_user</field>
        <field name="model_id" ref="asterisk_plus_phone.model_asterisk_plus_phone_favorite"/>
        <field name="group_id" ref="asterisk_plus.group_asterisk_user"/>
        <field name="perm_read" eval="1"/>
        <field name="perm_write" eval="1"/>
        <field name="perm_create" eval="1"/>
        <field name="perm_unlink" eval="1"/>
    </record>

</odoo>